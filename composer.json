{
  "name": "sebbaum/legal",
  "type": "library",
  "description": "A Laravel package to integrate and maintain legal requirements",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Sebastian Baum",
      "email": "seb.baum@googlemail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5.5",
    "graham-campbell/markdown": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Sebbaum\\Legal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sebbaum\\Legal\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sebbaum\\Legal\\LegalServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
